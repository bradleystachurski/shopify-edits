      });
    </script>
  {% endif %}
<script>
  $(document).ready(function() {
    $('.has-hover').hover( function(){
    	$(this).children('.overlay').removeClass('hidden');
      	$(this).children('.play').addClass('hover');
    }, function(){
    	$(this).children('.overlay').addClass('hidden');
      	$(this).children('.play').removeClass('hover');
    });
  });
</script>
