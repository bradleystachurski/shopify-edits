  <header class="main-header">
    <div class="bg"></div>
    <div class="row top">
      {% if settings.show_multiple_currencies and settings.header-logo-center %}
        <div class="currency-selector columns left-aligned">
          {% include 'snippet-currency-selector' %}
        </div>
      {% endif %}

      <div class="columns{% if settings.header-logo-center %} logo-center{% endif %}">
        {% include 'snippet-store-title' %}
      </div>

      <div class="search-account columns">
        {% include 'snippet-searchbar-account' %}
      </div>

    </div><!-- .row -->

    <div class="bottom-row">
      <div class="row">
        <div class="columns menu-container{% if settings.header-menu-center %} text-center{% endif %}">
          {% include 'snippet-main-menu' %}
        </div>

        <div class="columns cart-container">
          {% include 'snippet-cart' %}
        </div>

        <div class="mobile-tools">
          <a class="glyph menu" href=""></a>
          {% if settings.header-search-show %}
          <a href="/search" class="glyph fa fa-search"></a>
          {% endif %}
          {% if settings.header-account-show and shop.customer_accounts_enabled %}
          <a href="/account" class="glyph account"></a>
          {% endif %}
          <a href="/cart" class="glyph cart"></a>

        </div>
      </div>
    </div>

    <div class="main-menu-dropdown-panel">
      <div class="row{% if settings.header-menu-center %} text-center{% endif %}" sytle="font-size: 18px;">

      </div>
    </div>

    <div class="mobile-menu hide">
      {% include 'snippet-mobile-menu' %}
    </div>

    <div class="row">
      <div class="header-divider"></div>
    </div>

  </header>

  <section class="main-content">

      {{ content_for_layout }}
  </section>

  <footer class="main-footer">

    <div class="row bottom">
	  <div class="column-1 large-3 columns info">
        <h2 class="title">SYMBOLIC MUSCLE</h2>
        <div class="content">
          <ul class="footer-nav plain-list info" role="navigation">
            {% for link in linklists.info.links %}
            <li>
              <a href="{{ link.url }}">
                {{ link.title }}
              </a>
            </li>
            {% endfor %}
          </ul>
        </div>
      </div>
      <div class="column-2 large-3 columns">
        <h2 class="title">Customer Care</h2>
        <div class="content">
          <ul class="footer-nav plain-list customer-care" role="navigation">
            {% for link in linklists.care.links %}
            <li>
              <a href="{{ link.url }}">
                {{ link.title }}
              </a>
            </li>
            {% endfor %}
          </ul>
          
        </div>
      </div>
      <div class="column-3 large-3 columns">
        <h2 class="title">Contact Us</h2>
        <div class="content">
          <ul class="footer-nav plain-list contact-us" role="navigation">
            <li><a href="/pages/contact-us">Contact Form</a></li>
            <li><a href="tel:18556229100">1-855-622-9100</a></li>
            <li class="address">Symbolic Muscle LLC<br>8850 Business Park Dr<br>Suite 100<br>Austin, Texas 78759</li>
          </ul>
        </div>
      </div>
      <div class="column-4 large-3 columns">
          <ul class="footer-nav plain-list socal" role="navigation">
            <li class="footer-icon"><a href="{{settings['footer-facebook-follow-url']}}"><i class="fa fa-facebook"></i></a></li>
            <li class="footer-icon"><a href="{{settings['footer-twitter-follow-url']}}"><i class="fa fa-twitter"></i></a></li>
            {% comment %}<li class="footer-icon"><a href="{{settings['footer-google-follow-url']}}"><i class="fa fa-google"></i></a></li>{% endcomment %}
            {% comment %}<li class="footer-icon"><a href="{{settings['footer-pinterest-follow-url']}}"><i class="fa fa-pinterest-p"></i></a></li>
            <li class="footer-icon"><a href="{{settings['footer-email-follow-url']}}"><i class="fa fa-envelope"></i></a></li>{% endcomment %}
            <li class="footer-icon"><a href="{{settings['footer-linkedin-follow-url']}}"><i class="fa fa-linkedin"></i></a></li>
            <li class="footer-icon"><a href="{{settings['footer-instagram-follow-url']}}"><i class="fa fa-instagram"></i></a></li>
            
          </ul>
        	<ul class="footer-nav plain-list payment" role="navigation">
              <li class="footer-icon"><i class="fa fa-cc-amex"></i></li>
              <li class="footer-icon"><i class="fa fa-cc-mastercard"></i></li>
              <li class="footer-icon"><i class="fa fa-cc-visa"></i></li>
          	</ul>
      </div>

    </div>
    <div class="row copyright">
      <div class="row angle"></div>
      <p>Copyright © 2017 Symbolic Muscle</p>
    </div>
  </footer>

